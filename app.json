{
  "expo": {
    "name": "PicklePro",
    "slug": "picklepro-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#4F46E5",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.picklepro.mobile"
    },
    "android": {
      "backgroundColor": "#4F46E5",
      "package": "com.picklepro.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#4F46E5"
      },
      "permissions": []
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
