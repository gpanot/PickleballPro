{
  "expo": {
    "name": "PicklePro",
    "slug": "picklepro-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "pickleballhero",
    "splash": {
      "image": "./assets/images/splash_ball.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.picklepro.mobile",
      "icon": "./assets/images/icon.png",
      "associatedDomains": ["applinks:pickleballhero.app"],
      "config": {
        "googleMapsApiKey": "AIzaSyAMNdMtfFdyekAB1VwDORs9Fesl41JUajg"
      },
      "infoPlist": {
        "NSUserTrackingUsageDescription": "We use tracking to provide personalized coaching recommendations and improve your training experience."
      }
    },
    "android": {
      "backgroundColor": "#000000",
      "package": "com.picklepro.mobile",
      "versionCode": 11,
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "pickleballhero"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.CAMERA"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAMNdMtfFdyekAB1VwDORs9Fesl41JUajg"
        }
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-camera",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d9a056b2-6df1-44d9-bf47-7d928a2029b2"
      }
    }
  }
}